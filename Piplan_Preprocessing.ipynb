    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"from sklearn.pipeline import Pipeline\\n\",\n",
    "    \"from sklearn.preprocessing import OneHotEncoder\\n\",\n",
    "    \"from sklearn.compose import ColumnTransformer\\n\",\n",
    "    \"from sklearn.base import BaseEstimator, TransformerMixin\\n\",\n",
    "    \"\\n\",\n",
    "    \"df = pd.read_excel(\\\"C:/Users/SK-TECH/Downloads/household_new.xlsx\\\")\"\n",
    "   ],\n",
    "   \"id\": \"590151d55420b83f\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 1\n",
    "  {\n",
    "  },\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.748829Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.742906Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"columns_to_drop = [\\\"cet_cest_timestamp\\\",\\\"area_offices\\\",\\\"area_room_1\\\",\\\"area_room_2\\\",\\\"area_room_3\\\",\\\"area_room_4\\\",\\\"compressor\\\",\\\"cooling_aggregate\\\",\\\"cooling_pumps\\\",\\\"dishwasher\\\",\\\"ev\\\",\\\"grid_import\\\",\\\"pv_facade\\\",\\\"pv_roof\\\",\\\"refrigerator\\\",\\\"ventilation\\\"]\\n\",\n",
    "    \"training_columns = [\\\"utc_timestamp\\\",\\\"power_diff\\\", \\\"machine_1\\\", \\\"machine_2\\\", \\\"machine_3\\\", \\\"machine_4\\\", \\\"machine_5\\\"]\\n\",\n",
    "    \"normal_column = [\\\"utc_timestamp\\\",\\\"machine_col\\\" ,\\\"power\\\", \\\"machine_id\\\",\\\"power_diff\\\" ]\"\n",
    "   ],\n",
    "   \"id\": \"c6f3b970bfeaf87d\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 2\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.773947Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.764731Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class ColumnDropper(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"    def __init__(self, columns_to_drop):\\n\",\n",
    "    \"        # We store the parameters here\\n\",\n",
    "    \"        self.columns_to_drop = columns_to_drop\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        # Nothing to learn/calculate here, so we just return self\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        # Return the dataframe with columns dropped\\n\",\n",
    "    \"        return X.drop(columns=self.columns_to_drop)\"\n",
    "   ],\n",
    "   \"id\": \"78a23159c298c549\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 3\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.796788Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.791489Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class Drop_na(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        X=X.copy()\\n\",\n",
    "    \"        return X.dropna()\"\n",
    "   ],\n",
    "   \"id\": \"69fd86944ba614e6\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 4\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.819324Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.813344Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class Melt_data(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        X = X.copy()\\n\",\n",
    "    \"        return X.melt(\\n\",\n",
    "    \"            id_vars=[\\\"utc_timestamp\\\"],\\n\",\n",
    "    \"            value_vars= [\\\"machine_1\\\",\\\"machine_2\\\",\\\"machine_3\\\",\\\"machine_4\\\",\\\"machine_5\\\"],\\n\",\n",
    "    \"            var_name=\\\"machine_col\\\",\\n\",\n",
    "    \"            value_name=\\\"power\\\",\\n\",\n",
    "    \"        )\"\n",
    "   ],\n",
    "   \"id\": \"26db7f52fca46d0c\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 5\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.841251Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.834057Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class Sort(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        X = X.copy()\\n\",\n",
    "    \"        return X.sort_values(\\\"utc_timestamp\\\").reset_index(drop=True)\"\n",
    "   ],\n",
    "   \"id\": \"6d0b3cb50edf7762\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 6\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.857905Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.852270Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class Sort_For_Machine(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        X = X.copy()\\n\",\n",
    "    \"        return X.sort_values([\\\"machine_id\\\", \\\"utc_timestamp\\\"]).reset_index(drop=True)\"\n",
    "   ],\n",
    "   \"id\": \"ceaaa6cf6e03ef9a\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 7\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.878755Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.872375Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class Extract_machine_id(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        X = X.copy()\\n\",\n",
    "    \"        X[\\\"machine_id\\\"] = X[\\\"machine_col\\\"].str.replace(\\\"machine_\\\", \\\"\\\").astype(int)\\n\",\n",
    "    \"        return X\"\n",
    "   ],\n",
    "   \"id\": \"6ea59fc4de560aec\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 8\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.896032Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.888815Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class Calculate_power_diff(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        X = X.copy()\\n\",\n",
    "    \"        X[\\\"power_diff\\\"] = X.groupby(\\\"machine_id\\\")[\\\"power\\\"].diff()/0.25\\n\",\n",
    "    \"        X[\\\"power_diff\\\"] = X[\\\"power_diff\\\"].fillna(0)\\n\",\n",
    "    \"        return X\"\n",
    "   ],\n",
    "   \"id\": \"4aeb47441eae6e53\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 9\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.912521Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.906377Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class Parse_data(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        X = X.copy()\\n\",\n",
    "    \"        X['utc_timestamp'] = pd.to_datetime(X['utc_timestamp'], errors='coerce', utc=True)\\n\",\n",
    "    \"        return X\"\n",
    "   ],\n",
    "   \"id\": \"26c3a0aaceb5b939\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 10\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": [\n",
    "    \"# time scaling to hour and minute to have more meaningful features for the model, we will use the minute of the day as a feature, and we will scale it to be between 0 and 1\\n\",\n",
    "    \"import numpy as np\\n\",\n",
    "    \"class Time_Scaling(BaseEstimator, TransformerMixin):\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def fit(self, X, y=None):\\n\",\n",
    "    \"        return self\\n\",\n",
    "    \"\\n\",\n",
    "    \"    def transform(self, X):\\n\",\n",
    "    \"        X = X.copy()\\n\",\n",
    "    \"        X[\\\"hour\\\"] = X[\\\"utc_timestamp\\\"].dt.hour\\n\",\n",
    "    \"        X[\\\"hour_cos\\\"] = np.cos(round(2 * np.pi * X[\\\"hour\\\"] / 24, 4))\\n\",\n",
    "    \"        X[\\\"hour_sin\\\"] = np.sin(round(2 * np.pi * X[\\\"hour\\\"] / 24, 4))\\n\",\n",
    "    \"        X[\\\"is_night\\\"] = ((X[\\\"hour\\\"] >= 20) | (X[\\\"hour\\\"] < 6)).astype(int)\\n\",\n",
    "    \"        return X\\n\"\n",
    "   ],\n",
    "   \"id\": \"d8990517712582d\"\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.930065Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.921704Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"ct = ColumnTransformer(\\n\",\n",
    "    \"    transformers=[\\n\",\n",
    "    \"        ('passthrough', 'passthrough', ['utc_timestamp', 'power_diff']),\\n\",\n",
    "    \"        ('machine', OneHotEncoder(\\n\",\n",
    "    \"            handle_unknown='ignore', sparse_output=False  # Dense for sanity\\n\",\n",
    "    \"        ), ['machine_id'])\\n\",\n",
    "    \"    ],\\n\",\n",
    "    \"    remainder='drop'\\n\",\n",
    "    \")\"\n",
    "   ],\n",
    "   \"id\": \"f4a60866a8872450\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 11\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.977056Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.944566Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"# transform the data using the pipeline in function to used in other scripts\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"My_Pipeline = Pipeline([\\n\",\n",
    "    \"    (\\\"drop_columns\\\", ColumnDropper(columns_to_drop)),  # placeholder for column dropping\\n\",\n",
    "    \"    (\\\"parse_dates\\\", Parse_data()),    # placeholder for date parsing     # placeholder for sorting\\n\",\n",
    "    \"    (\\\"melt_data\\\",Melt_data() ),      # placeholder for melting\\n\",\n",
    "    \"    (\\\"drop_na\\\",Drop_na()),          # placeholder for dropping NA values\\n\",\n",
    "    \"    # extract machine_id before sorting by machine_id\\n\",\n",
    "    \"    (\\\"extract_machine_id\\\", Extract_machine_id()),  # placeholder for extracting machine_id\\n\",\n",
    "    \"    (\\\"sort_for_machine\\\", Sort_For_Machine()),  # placeholder for sorting by machine_id and timestamp\\n\",\n",
    "    \"    (\\\"calculate_power_diff\\\", Calculate_power_diff()),  # placeholder for calculating power_diff\\n\",\n",
    "    \"    (\\\"final_sort\\\", Sort()),  # run final DataFrame sort BEFORE encoding so Sort sees a DataFrame\\n\",\n",
    "    \"])\"\n",
    "   ],\n",
    "   \"id\": \"f3cdd704c647da2b\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 12\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:00.990680Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:00.986155Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"ct = ColumnTransformer(\\n\",\n",
    "    \"    transformers=[\\n\",\n",
    "    \"        ('passthrough', 'passthrough', ['utc_timestamp', 'power_diff']),\\n\",\n",
    "    \"        ('machine', OneHotEncoder(\\n\",\n",
    "    \"            handle_unknown='ignore', sparse_output=False  # Dense for sanity\\n\",\n",
    "    \"        ), ['machine_id'])\\n\",\n",
    "    \"    ],\\n\",\n",
    "    \"    remainder='drop'\\n\",\n",
    "    \")\"\n",
    "   ],\n",
    "   \"id\": \"f4a68cb4c4172b2d\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": 13\n",
    "  },\n",
    "  {\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2026-02-10T00:16:01.007647Z\",\n",
    "     \"start_time\": \"2026-02-10T00:16:01.001694Z\"\n",
    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"Final_Pipeline = Pipeline([\\n\",\n",
    "    \"    (\\\"first_pipeline\\\", My_Pipeline),\\n\",\n",
    "    \"    (\\\"column_transformer\\\", ct),\\n\",\n",
    "    \"])\"\n",
    "   ],\n",
